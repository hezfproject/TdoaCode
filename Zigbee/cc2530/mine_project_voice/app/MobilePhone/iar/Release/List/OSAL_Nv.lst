    171          
    172          // Offset into the page of the first available erased space.
    173          static uint16 pgOff[OSAL_NV_PAGES_USED];
    174          
    175          // Count of the bytes lost for the zeroed-out items.
    176          static uint16 pgLost[OSAL_NV_PAGES_USED];
