     76          /*********************************************************************
     77          * @fn      osalInitTasks
     78          *
     79          * @brief   This function invokes the initialization function for each task.
     80          *
     81          * @param   void
     82          *
     83          * @return  none
     84          */
     85          void osalInitTasks( void )
     86          {
     87          	uint8 taskID = 0;
     88          
     89          	tasksEvents = (uint16 *)osal_mem_alloc( sizeof( uint16 ) * tasksCnt);
     90          	osal_memset( tasksEvents, 0, (sizeof( uint16 ) * tasksCnt));
     91          
     92          	macTaskInit( taskID++ );
     93          	MP_Init( taskID++ );
     94          	Hal_Init( taskID++ );
     95          	MP_Function_Init(taskID++);
     96          }
