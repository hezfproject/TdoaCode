    133           * EXTERNAL FUNCTIONS
    134           */
    135          
    136          /*********************************************************************
    137           * LOCAL VARIABLES
    138           */
    139          
    140          static osalMemHdr_t *ff1;  // First free block in the small-block bucket.
    141          static osalMemHdr_t *ff2;  // First free block after the small-block bucket.
    142          
    143          #if ( OSALMEM_METRICS )
    144            static uint16 blkMax;  // Max cnt of all blocks ever seen at once.
    145            static uint16 blkCnt;  // Current cnt of all blocks.
    146            static uint16 blkFree; // Current cnt of free blocks.
    147            static uint16 memAlo;  // Current total memory allocated.
    148            static uint16 memMax;  // Max total memory ever allocated at once.
    149          #endif
    150          
    151          #if ( OSALMEM_PROFILER )
    152            #define OSALMEM_PROMAX  8
    153            /* The profiling buckets must differ by at least OSALMEM_MIN_BLKSZ; the
    154             * last bucket must equal the max alloc size. Set the bucket sizes to
    155             * whatever sizes necessary to show how your application is using memory.
    156             */
    157            static uint16 proCnt[OSALMEM_PROMAX] = {
    158              OSALMEM_SMALL_BLKSZ, 48, 112, 176, 192, 224, 256, 65535 };
    159            static uint16 proCur[OSALMEM_PROMAX] = { 0 };
    160            static uint16 proMax[OSALMEM_PROMAX] = { 0 };
    161            static uint16 proTot[OSALMEM_PROMAX] = { 0 };
    162            static uint16 proSmallBlkMiss;
    163          #endif
    164          
    165          // Memory Allocation Heap.
    166          #if defined( EXTERNAL_RAM )
    167            static uint8 *theHeap = (uint8 *)EXT_RAM_BEG;
    168          #else
    169            static halDataAlign_t _theHeap[ MAXMEMHEAP / sizeof( halDataAlign_t ) ];
    170            static uint8 *theHeap = (uint8 *)_theHeap;
    171          #endif
